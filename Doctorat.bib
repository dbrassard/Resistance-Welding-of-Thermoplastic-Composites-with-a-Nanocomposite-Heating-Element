file = {:home/david/Documents/Travaux/Doctorat/Mendeley/Rudolf, Mitschang, Neitzel/Composites Part A Applied Science and Manufacturing/Rudolf, Mitschang, Neitzel - 2000 - Induction heating of continuous carbon-fibre-reinforced thermoplastics.pdf:pdf},
