file = {:home/david/Documents/Travaux/Doctorat/Mendeley/Jin et al/European Polymer Journal/Jin et al. - 2013 - Enhancing the electrical conductivity of polymer composites.pdf:pdf},
isbn = {0014-3057},
issn = {00143057},
journal = {Eur. Polym. J.},
keywords = {Composite,Electrical conductivity,Nanofiller,Tunnelling},
number = {5},
pages = {1066--1072},
publisher = {Elsevier Ltd},
title = {{Enhancing the electrical conductivity of polymer composites}},
url = {http://dx.doi.org/10.1016/j.eurpolymj.2013.01.014},
volume = {49},
year = {2013}
}
