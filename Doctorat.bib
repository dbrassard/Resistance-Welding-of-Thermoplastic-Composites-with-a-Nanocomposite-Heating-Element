doi = {10.1021/nn2041494},
file = {:home/david/Documents/Travaux/Doctorat/Mendeley/Fogden et al/ACS Nano/Fogden et al. - 2012 - Scalable method for the reductive dissolution, purification, and separation of single-walled carbon nanotubes.pdf:pdf},
