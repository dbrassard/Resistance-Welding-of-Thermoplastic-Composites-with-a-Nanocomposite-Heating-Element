doi = {10.1016/j.powtec.2013.02.024},
file = {:home/david/Documents/Travaux/Doctorat/Mendeley/Dorval Dion, Tavares/Powder Technology/Dorval Dion, Tavares - 2013 - Photo-initiated chemical vapor deposition as a scalable particle functionalization technology (a practical.pdf:pdf},
