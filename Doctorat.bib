doi = {10.1016/S0263-8223(00)00047-7},
file = {:home/david/Documents/Travaux/Doctorat/Mendeley/Hou et al/Composite Structures/Hou et al. - 1999 - Resistance welding of carbon fibre reinforced thermoplastic composite using alternative heating element.pdf:pdf},
