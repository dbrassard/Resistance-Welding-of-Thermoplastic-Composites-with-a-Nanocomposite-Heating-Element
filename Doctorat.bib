journal = {Prog. Polym. Sci.},
keywords = {Conductive network,Conductive polymer composites,Electrical percolation threshold,Electroactive multifunctional,Morphological control,Sensing},
number = {4},
pages = {627--655},
publisher = {Elsevier Ltd},
title = {{Progress on the morphological control of conductive network in conductive polymer composites and the use as electroactive multifunctional materials}},
volume = {39},
