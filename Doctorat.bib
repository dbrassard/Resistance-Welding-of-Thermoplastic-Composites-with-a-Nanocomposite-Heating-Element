doi = {10.1016/j.progpolymsci.2009.09.003},
file = {:home/david/Documents/Travaux/Doctorat/Mendeley/Spitalsky et al/Progress in Polymer Science (Oxford)/Spitalsky et al. - 2010 - Carbon nanotube-polymer composites Chemistry, processing, mechanical and electrical properties.pdf:pdf},
